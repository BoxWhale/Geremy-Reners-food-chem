@page
@model ResultsModel
@{
    ViewData["Title"] = "Results page";
}
<main role="main">
    <div class="CheckAnswers addjuster">
        <p id="displayanswer"> </p>
    </div>
</main>



<script>
    // Call allStorage and checkAnswers when the window finishes loading so the DOM elements exist.
    window.addEventListener('load', () => {
        // Only run the answers check on this page; do not show raw storage keys
        if (typeof checkAnswers === 'function') {
            checkAnswers();
        }
    });
</script>